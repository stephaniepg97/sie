    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.intervencao ){",
        "\treturn {",
        "\t\t//map intervencao variable to expected task contract input",
        "\t\tintervencaoInput: {",
        "\t\t\tvalor: $data.intervencao.valor !== undefined ? $data.intervencao.valor : null,",
        "\t\t\tobs: $data.intervencao.obs !== undefined ? $data.intervencao.obs : null,",
        "\t\t\ttitulo: $data.intervencao.titulo !== undefined ? $data.intervencao.titulo : null,",
